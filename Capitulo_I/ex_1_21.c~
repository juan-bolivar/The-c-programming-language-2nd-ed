#include <stdio.h>

#define n_cols 8



main(){
  int c; 
  int size_spaces ;
  int number_spaces;
  int number_tabs;
  int i;

  
  while (( c = getchar()) != EOF){
    if( c !=' '){

      putchar(c);

    }

    else{
      size_spaces = 1; 
      while( ( c = getchar() )== ' ' && c!='\n' ){

	++size_spaces;

      }
      number_spaces = size_spaces % n_cols;
      number_tabs   = size_spaces / n_cols;
	
      for (i = 0 ; i <= number_tabs ; i++){
	  
	putchar('\t');
      }

      for ( i = 0 ; i<= number_spaces ; i++) {
	putchar(' ');
      }
      
    }
  }
  return 0;
}


